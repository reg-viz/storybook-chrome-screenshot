    "@storybook/addon-links": "^4.0.0",
    "@storybook/addon-notes": "^4.0.0",
    "@storybook/addon-viewport": "^4.0.0",
    "@storybook/addons": "^4.0.0",
    "@storybook/vue": "^4.0.0",
    "babel-loader": "8.0.4",
    "babel-preset-vue": "^2.0.2",
